Number in_1_Temperature  "Студен  1 [%.1f °C]"                   <temperature>  (gIn_1, gPersist_On_Change_1minute)                                                      { channel="mqtt:topic:localMQTTbroker:in_1:temperature" }
Number in_1_Humidity     "Студен  1 [%.1f %%]"                   <humidity>     (gIn_1, gPersist_On_Change_1minute)                                                      { channel="mqtt:topic:localMQTTbroker:in_1:humidity" }
