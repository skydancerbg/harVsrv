Group g1_btn_lights
Group g1_tnl_lights
Group g1AllLights 

Group gIn_1 
Group gIn_1_lights
Switch in_1_button       "Тунел 1 LED"                         <switch>       (gIn_1, gPersist_On_Change_1minute)                                                                                 { channel="mqtt:topic:localMQTTbroker:in_1:button" }       
Switch in_1_tnl_light    "Тунел 1 LED"                         <switch>       (gIn_1, gIn_1_lights, g1_tnl_lights, g1AllLights, gTnlLights)                               { channel="mqtt:topic:localMQTTbroker:in_1:power1" }       
